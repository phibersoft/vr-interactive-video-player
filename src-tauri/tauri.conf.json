{
    "$schema": "../node_modules/@tauri-apps/cli/schema.json",
    "build": {
        "beforeBuildCommand": "npm run client:build",
        "beforeDevCommand": "npm run client:dev",
        "devPath": "http://localhost:3000",
        "distDir": "../out"
    },
    "package": {
        "productName": "vr-interactive-video-player",
        "version": "1.0.0"
    },
    "tauri": {
        "allowlist": {
            "window": {
                "all": true
            }
        },
        "bundle": {
            "active": true,
            "category": "DeveloperTool",
            "copyright": "",
            "deb": {
                "depends": []
            },
            "externalBin": [],
            "icon": [
                "icons/favicon.ico"
            ],
            "identifier": "com.vr-interactive-video-player.phibersoft",
            "longDescription": "",
            "macOS": {
                "entitlements": null,
                "exceptionDomain": "",
                "frameworks": [],
                "providerShortName": null,
                "signingIdentity": null
            },
            "resources": [],
            "shortDescription": "",
            "targets": "all",
            "windows": {
                "certificateThumbprint": null,
                "digestAlgorithm": "sha256",
                "timestampUrl": ""
            }
        },
        "security": {
            "csp": null
        },
        "updater": {
            "active": false
        },
        "windows": [
            {
                "fullscreen": false,
                "height": 600,
                "resizable": true,
                "title": "VR Interactive Video Player",
                "width": 800,
                "center": true
            }
        ]
    }
}